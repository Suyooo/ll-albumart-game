{
  "name": "ll-albumart",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.config.site.ts --host 0.0.0.0",
    "build": "DEBUG=vite-tsconfig-paths vite --config vite.config.site.ts build && vite --config vite.config.makeShare.ts build",
    "preview": "vite --config vite.config.site.ts --host 0.0.0.0 preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "svelte-check --fail-on-hints --tsconfig ./tsconfig.json",
    "download-albums": "ts-node --esm \"scripts/downloadAlbums.ts\"",
    "test-albums": "ts-node --esm \"scripts/testAlbums.ts\""
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.1",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.12",
    "axios": "^1.1.3",
    "canvas": "~2.10.0",
    "fuzzysort": "^2.0.1",
    "postcss": "^8.4.18",
    "postcss-load-config": "^4.0.1",
    "rollup-plugin-tsconfig-paths": "^1.3.0",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.3",
    "svelte-reduced-motion": "^1.0.5",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.4.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vite-tsconfig-paths": "^4.0.0"
  },
  "dependencies": {
    "focus-trap": "^7.0.0"
  }
}
